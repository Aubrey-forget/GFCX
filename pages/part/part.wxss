.head {
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
  margin-left: 50rpx;
}

.head image {
  width: 280rpx;
  height: 220rpx;
  margin-right: 50rpx;
}

.details {
  /* margin-bottom: 10rpx; */
  font-size: 48rpx;
  /* font-weight: bold; */
}

#star {
  height: 50rpx;
  width: 234rpx;
  margin-left: -20rpx;
  display: block;
}

.media {
  font-size: 32rpx;
  color: #808080;
  display: inline-block;
}

.mold {
  display: flex;
  flex-direction: row;
}

.mold view {
  background: #eee;
  margin-right: 20rpx;
  border-radius: 10rpx;
    padding: 0rpx 18rpx 8rpx 18rpx;
}

.mold view text {
  font-size: 26rpx;
  /* display: inline-block; */

  color: #b4b4b4;
}

.cycle {
  margin-top: 50rpx;
  margin-bottom: 30rpx;
  width: 100%;
  height: 400rpx;
}

.cycle swiper-item image {
  width: 100%;
  height: 500rpx;
}

#content {
  padding: 0 40rpx;
}

.list {
  border-top: 1px solid #eaeaea;
}

.text {
  display: inline-block;
  margin-left: 50rpx;
  margin-top: 20rpx;
  padding-bottom: 50rpx;
  font-size: 30rpx;
  color: #909090;
}

.img {
  width: 26rpx;
  height: 26rpx;
}
