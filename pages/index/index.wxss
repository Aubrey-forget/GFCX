
.contact {
  display: flex;
  background: #000;
  padding: 15rpx 0;
}

.aboutImg {
  width: 54rpx;
  height: 46rpx;
  margin-top: 4%;
  margin-left: 12%;
  margin-right: 2%;
}

.connectImg {
  width: 48rpx;
  height: 54rpx;
  margin-top: 4%;
  margin-left: 10%;
  margin-right: 2%;
}

.regard {
  width: 29%;
}

.rightLine {
  border-right: 1rpx solid #575e69;
}

.regard text:nth-of-type(1) {
  color: white;
  font-size: 38rpx;
}

.regard text:nth-of-type(2) {
  font-size: 26rpx;
  color: #7d848e;
}

.case {
  padding: 10rpx 30rpx;
}

.boutique {
  margin-top: 20rpx;
}

.boutique>view:nth-of-type(1) {
  display: flex;
  flex-direction: column;
}

.boutique view:nth-of-type(1) text:nth-of-type(1) {
  font-size: 48rpx;
  color: black;
  font-family: '黑体';
}

.boutique view:nth-of-type(1) text:nth-of-type(2) {
  color: #7d848e;
  font-size: 24rpx;
  font-family: '黑体';
}

.cas {
  display: flex;
  justify-content: flex-start;
  flex-flow: row wrap;
  padding: 15rpx 0 0 60rpx;
}
.cas navigator{
  margin-right: 8%;
}

.case1 {
  margin-bottom: 15rpx;
  text-align: center;
}


.img {
  width: 260rpx;
  height: 160rpx;
}

.case1 text {
  font-size: 32rpx;
  display: block;
  margin-bottom: 30rpx;
  font-family: '黑体';
}

#box {
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

#box view {
  width: 20%;
  margin-left: 4%;
  text-align: center;
}

#box view image {
  width: 100rpx;
  height: 100rpx;
}

#box view text {
  font-size: 32rpx;
  font-weight: normal;
  font-family: "黑体";
}
